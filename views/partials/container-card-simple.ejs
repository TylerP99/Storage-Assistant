<% for(let i = 0; i < containers.length; ++i) { %>


    <section class="container-card-simple">

        <h3><%= containers[i].name %></h3>

        <p><%=containers[i].description%></p>

        <a class="view-button" href="/container/<%=containers[i]._id%>">View Container</a>

    </section>


<% } %>